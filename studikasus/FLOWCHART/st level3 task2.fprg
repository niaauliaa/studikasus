<flowgorithm fileversion="3.0">
    <attributes>
        <attribute name="name" value=""/>
        <attribute name="authors" value="Septi Aulia"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2023-03-08 08:55:49 AM"/>
        <attribute name="created" value="U2VwdGkgQXVsaWE7SFAtU0VQVEk5OzIwMjMtMDMtMDg7MDg6NDg6NDIgQU07Mjk1Nw=="/>
        <attribute name="edited" value="U2VwdGkgQXVsaWE7SFAtU0VQVEk5OzIwMjMtMDMtMDg7MDg6NTU6NDkgQU07MjszMDcx"/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="ang1, ang2, ang3, mid" type="Integer" array="False" size=""/>
            <output expression="&quot;masukkan ang ke-1&quot;" newline="True"/>
            <input variable="ang1"/>
            <output expression="&quot;masukkan ang ke-2&quot;" newline="True"/>
            <input variable="ang2"/>
            <output expression="&quot;masukkan ang ke-3&quot;" newline="True"/>
            <input variable="ang3"/>
            <if expression="ang1&gt;ang2">
                <then>
                    <if expression="ang2&gt;ang3">
                        <then>
                            <assign variable="mid" expression="ang2"/>
                        </then>
                        <else>
                            <if expression="ang1&gt;ang3">
                                <then>
                                    <assign variable="mid" expression="ang3"/>
                                </then>
                                <else>
                                    <assign variable="mid" expression="ang1"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </then>
                <else>
                    <if expression="ang1&gt;ang3">
                        <then>
                            <assign variable="mid" expression="ang1"/>
                        </then>
                        <else>
                            <if expression="ang2&gt;ang3">
                                <then>
                                    <assign variable="mid" expression="ang3"/>
                                </then>
                                <else>
                                    <assign variable="mid" expression="ang2"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="&quot;Bilangan dengan nilai tengah&quot; &amp;mid" newline="True"/>
        </body>
    </function>
</flowgorithm>
